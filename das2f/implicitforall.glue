Main [ @sharps kquery Match @Ensures] = [[\n${sharps} query ${Match}\n${Ensures}]]

Match [line] = [[${line}]]
Ensure [line] = [[${line}]]

line [@notNL nl] = [[${notNL}${nl}]]

notPAREN [c] = [[${c}]]

ident [firstChar @restChar] = [[${firstChar}${restChar}]]
firstChar [c] = [[${c}]]
restChar [c] = [[${c}]]
nl  [c] = [[${c}]]
sharp  [c] = [[${c}]]
notNL  [c] = [[${c}]]
ws  [c] = [[${c}]]

